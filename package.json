{
	"name": "fast-squaring",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --open",
		"upload": "vite build && gh-pages --dotfiles -d build",
		"build": "vite build",
		"preview": "vite preview --open",
		"sync": "git add * && git commit -m 'xf'&& git pull && git push && npm run upload" 
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"gh-pages": "^5.0.0",
		"html-minifier": "^4.0.0",
		"svelte": "^3.54.0",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module"
}
